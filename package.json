{
  "name": "modern-webapp",
  "version": "0.0.1",
  "description": "A configuration boilerplate for a modern frontend SPA workflow.",
  "scripts": {
    "build:dev": "webpack --env=dev --progress --colors",
    "build:dist": "webpack -p --env=prod --progress --colors",
    "serve:dev": "webpack-dev-server --env=dev --progress --colors",
    "serve:dist": "webpack-dev-server -p --env=prod --progress --colors",
    "lint": "eslint src",
    "precommit": "eslint --max-warnings 0 src"
  },
  "keywords": [
    "react",
    "router",
    "redux",
    "postcss",
    "precss",
    "css modules",
    "webpack",
    "babel",
    "eslint",
    "stylelint",
    "Semantic UI"
  ],
  "author": {
    "name": "Robert Matych",
    "email": "robertsmatych@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rmatych/Modern-Webapp"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-loader": "7.1.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "browserslist": "^2.1.5",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "eslint": "4.1.0",
    "eslint-loader": "1.8.0",
    "eslint-plugin-import": "2.6.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-optimize-regex": "1.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "2.29.0",
    "husky": "0.14.1",
    "postcss": "^6.0.4",
    "postcss-css-reset": "^1.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "style-loader": "^0.18.2",
    "webpack": "3.0.0",
    "webpack-dev-server": "2.5.0",
    "webpack-merge": "4.1.0"
  },
  "dependencies": {
    "preact": "^8.1.0",
    "preact-compat": "^3.16.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
